ableOpacity\n        style={styles.btn}\n        onPress={async (): Promise<any> => {\n          await search()\n        }}\n      >\n        <Text style={styles.text}>Search</Text>\n      </TouchableOpacity>\n      <TouchableOpacity\n        style={styles.btn}\n        onPress={(): any => {\n          setMode(mode === 'Grid' ? 'List' : 'Grid')\n        }}\n      >\n        <Text style={styles.text}>{mode === 'Grid' ? 'List' : 'Grid'}</Text>\n      </TouchableOpacity>\n\n      <View style={styles.content}>\n        {loding ? (\n          <View style={styles.indicator}>\n            <ActivityIndicator size={'large'} color={'#2fcccc'} />\n          </View>\n        ) : res.length > 0 ? (\n          mode === 'Grid' ? (\n            <Grid data={res} navigation={navigation} />\n          ) : (\n            <List data={res} navigation={navigation} />\n          )\n        ) : (\n          <Text style={styles.text}>no res</Text>\n        )}\n      </View>\n    </View>\n  )\n}\n\nconst styles = StyleSheet.create({\n  input: {\n    height: 60,\n    padding: 8,\n    fontSize: 16,\n    textAlign: 'center'\n  },\n  text: {\n    color: 'black',\n    fontSize: 20,\n    textAlign: 'center'\n  },\n  btn: {\n    color: '#a3a1a1',\n    padding: 5,\n    margin: 3\n  },\n  content: {\n    flex: 1\n  },\n  main: {\n    flex: 1\n  }\n})\n\nexport default Home\n","/Users/oreitan/Desktop/Github/reactNative/HomeWorkOne/component/ImageC.tsx",[],"/Users/oreitan/Desktop/Github/reactNative/HomeWorkOne/vmode/Grid.tsx",[],"/Users/oreitan/Desktop/Github/reactNative/HomeWorkOne/vmode/List.tsx",[],"/Users/oreitan/Desktop/Github/reactNative/HomeWorkOne/vmode/gridItem.tsx",[],"/Users/oreitan/Desktop/Github/reactNative/HomeWorkOne/vmode/listItem.tsx",[],{"ruleId":"37","severity":2,"message":"38","line":58,"column":27,"nodeType":null,"endLine":58,"endColumn":28,"fix":"39"},{"ruleId":"37","severity":2,"message":"40","line":59,"column":7,"nodeType":null,"endLine":59,"endColumn":7,"fix":"41"},"prettier/pre